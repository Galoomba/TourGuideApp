<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.starhood.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenu.Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".CountryMenu.MainActivity"
            android:label="choose Government"
            android:parentActivityName=".MainMenu.Menu" />

        <activity
            android:name=".HolderActivity.AttractionHolder"
            android:label="Attractions and Musumes"></activity>

        <activity
            android:name=".HolderActivity.NightClubs"
            android:label="NightClubs and Resturant"></activity>
        <activity
            android:name=".HolderActivity.HotlesClubs"
            android:label="Hotles"></activity>
    </application>

</manifest>